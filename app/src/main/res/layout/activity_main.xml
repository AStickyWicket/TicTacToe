<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="68dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/grid" />

    <TextView
        android:id="@+id/txvBottomRight"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="205dp"
        android:gravity="center"
        android:text="X"
        android:textColor="#000000"
        android:textSize="80sp"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txvBottomMiddle"
        app:layout_constraintTop_toBottomOf="@+id/txvMiddleRight" />

    <TextView
        android:id="@+id/txvBottomMiddle"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="205dp"
        android:gravity="center"
        android:text="X"
        android:textColor="#000000"
        android:textSize="80sp"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txvBottomRight"
        app:layout_constraintStart_toEndOf="@+id/txvBottomLeft"
        app:layout_constraintTop_toBottomOf="@+id/Middle" />

    <TextView
        android:id="@+id/txvBottomLeft"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="38dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="205dp"
        android:gravity="center"
        android:text="X"
        android:textColor="#000000"
        android:textSize="80sp"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txvBottomMiddle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txvMiddleLeft" />

    <TextView
        android:id="@+id/txvMiddleRight"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="26dp"
        android:gravity="center"
        android:text="X"
        android:textColor="#000000"
        android:textSize="80sp"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toTopOf="@+id/txvBottomRight"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Middle"
        app:layout_constraintTop_toBottomOf="@+id/txvTopRight" />

    <TextView
        android:id="@+id/Middle"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="29dp"
        android:gravity="center"
        android:text="X"
        android:textColor="#000000"
        android:textSize="80sp"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toTopOf="@+id/txvBottomMiddle"
        app:layout_constraintEnd_toStartOf="@+id/txvMiddleRight"
        app:layout_constraintStart_toEndOf="@+id/txvMiddleLeft"
        app:layout_constraintTop_toBottomOf="@+id/txvTopMiddle" />

    <TextView
        android:id="@+id/txvMiddleLeft"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="38dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="29dp"
        android:gravity="center"
        android:text="X"
        android:textColor="#000000"
        android:textSize="80sp"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toTopOf="@+id/txvBottomLeft"
        app:layout_constraintEnd_toStartOf="@+id/Middle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txvTopLeft" />

    <TextView
        android:id="@+id/txvTopRight"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="197dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="33dp"
        android:gravity="center"
        android:text="X"
        android:textColor="#000000"
        android:textSize="80sp"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toTopOf="@+id/txvMiddleRight"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txvTopMiddle"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txvTopMiddle"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="197dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="30dp"
        android:gravity="center"
        android:text="X"
        android:textColor="#000000"
        android:textSize="80sp"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toTopOf="@+id/Middle"
        app:layout_constraintEnd_toStartOf="@+id/txvTopRight"
        app:layout_constraintStart_toEndOf="@+id/txvTopLeft"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txvTopLeft"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="197dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="30dp"
        android:gravity="center"
        android:text="X"
        android:textColor="#000000"
        android:textSize="80sp"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toTopOf="@+id/txvMiddleLeft"
        app:layout_constraintEnd_toStartOf="@+id/txvTopMiddle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>